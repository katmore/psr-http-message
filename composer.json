{
	"name": "katmore/psr7-http",
	"description": "composer.json",
	"homepage": "https://github.com/katmore/psr7-http",
	"license": [
		"MIT",
		"GPL-3.0+"
	],
	"require": {
		"php": ">=7.2.1",
		"psr/http-message": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"Psr7Http\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Psr7Http\\TestCase\\": "tests/phpunit/TestCase",
			"Psr7Http\\Tests\\": "tests/phpunit/Tests"
		}
	},
	"require-dev": {
		"phpunit/phpunit": "^7",
		"phpunit/php-code-coverage": "^6.0",
		"php-di/phpdoc-reader": "^2.1"
	}
}